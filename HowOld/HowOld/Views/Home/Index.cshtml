            <img src="Resources/faces/scroll003.jpg" data-url="Resources/faces/main003.jpg" data-image-name="faces/main003.jpg" id="faces/main003.jpg" class="">
            <img src="Resources/faces/scroll004.jpg" data-url="Resources/faces/main004.jpg" data-image-name="faces/main004.jpg" id="faces/main004.jpg" class="selectedImage">
            <img src="Resources/faces/scroll005.jpg" data-url="Resources/faces/main005.jpg" data-image-name="faces/main005.jpg" id="faces/main005.jpg">
            <img src="Resources/faces/scroll007.jpg" data-url="Resources/faces/main007.jpg" data-image-name="faces/main007.jpg" id="faces/main007.jpg" class="">
            <img src="Resources/faces/scroll006.jpg" data-url="Resources/faces/main006.jpg" data-image-name="faces/main006.jpg" id="faces/main006.jpg" class="">
            <img src="Resources/faces/scroll008.jpg" data-url="Resources/faces/main008.jpg" data-image-name="faces/main008.jpg" id="faces/main008.jpg">
            <img src="Resources/faces/scroll009.jpg" data-url="Resources/faces/main009.jpg" data-image-name="faces/main009.jpg" id="faces/main009.jpg">
            <img src="Resources/faces/scroll0010.jpg" data-url="Resources/faces/main0010.jpg" data-image-name="faces/main0010.jpg" id="faces/main0010.jpg">
            <img src="Resources/faces/scroll0011.jpg" data-url="Resources/faces/main0011.jpg" data-image-name="faces/main0011.jpg" id="faces/main0011.jpg">
            <img src="Resources/faces/scroll0012.jpg" data-url="Resources/faces/main0012.jpg" data-image-name="faces/main0012.jpg" id="faces/main0012.jpg">
            <img src="Resources/faces/scroll0013.jpg" data-url="Resources/faces/main0013.jpg" data-image-name="faces/main0013.jpg" id="faces/main0013.jpg">
            <img src="Resources/faces/scroll0014.jpg" data-url="Resources/faces/main0014.jpg" data-image-name="faces/main0014.jpg" id="faces/main0014.jpg">
        </div>

        <div id="SelectorBox"></div>
        <div id="SelectorTag">
            <button type="button" class="btn btn-default" onclick=" analyzeUrl() ">
                <span class="glyphicon glyphicon-circle-arrow-right" style="margin-right: 4px" aria-hidden="true"></span>试试这张照片
            </button>
        </div>
        <div class="iScrollHorizontalScrollbar iScrollLoneScrollbar" style="position: absolute; z-index: 9999; height: 7px; left: 2px; right: 2px; bottom: 0px; overflow: hidden; pointer-events: none;"><div class="iScrollIndicator" style="box-sizing: border-box; position: absolute; border: 1px solid rgba(255, 255, 255, 0.901961); border-radius: 3px; height: 100%; transition-duration: 0ms; -webkit-transition-duration: 0ms; display: block; width: 13px; transform: translate(52px, 0px) translateZ(0px); transition-timing-function: cubic-bezier(0.1, 0.57, 0.1, 1); -webkit-transition-timing-function: cubic-bezier(0.1, 0.57, 0.1, 1); background: rgba(0, 0, 0, 0.498039);"></div></div>
    </div>


    <label id="uploadLabel" class="center-block" style="width: 230px;">
        <button id="uploadFileId" class="btn btn-default center-block action" style="background-color: rgb(174, 117, 11); border-radius: 0" onclick=" document.getElementById('uploadBtn').click(); ">
            <span>
                <img src="/Resources/browse-icon.svg" style="width: 25px; height: 25px">
            </span>
            上传你的图片
        </button>
        <input id="uploadBtn" type="file" accept="image/*" style="visibility: hidden; width: 0px; height: 0px">
    </label>

    <p id="maxImageSize" class="help-block center-block bodyfont">我们保证不会存储你上传的图片</p>
