<div id="selectImage">
    <form role="search" id="searchform" onsubmit="return searchImages()">
        <div class="input-group" style="background-color: white">
            <span class="input-group-addon" style="padding: 0px 0px 0px 0px; border: none"><img id="bLogo" class="" src="/Resources/bing2.png" data-bm="3"></span>
